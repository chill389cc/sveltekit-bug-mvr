{
  "name": "sveltekit-example",
  "description": "An example package in a npm workspaces monorepo",
  "version": "1.0.0",
  "dependencies": {
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/kit": "1.23.0",
    "svelte": "4.2.0",
    "svelte-check": "3.5.0",
    "typescript": "5.2.2",
    "vite": "4.4.9"
  }
}
